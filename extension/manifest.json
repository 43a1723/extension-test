{
  "manifest_version": 3,
  "name": "Web Action Extension",
  "description": "Run different scripts based on the website.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "https://discord.com/*",
    "https://roblox.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
