{
  "manifest_version": 3,
  "name": "Tên của extension của bạn",
  "version": "1.0",
  "permissions": [
    "tabs",
    "cookies",
    "webNavigation",
    "activeTab",
    "scripting",
    "https://*/*"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
